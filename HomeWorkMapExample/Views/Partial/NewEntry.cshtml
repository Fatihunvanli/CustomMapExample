<form>
    <div class="form-group">
        <label for="InputName">Adı</label>
        <input type="text" class="form-control" style="background:white;" id="InputName" placeholder="Ad Gir" autocomplete="off"/>
        <span id="alertInputName" style="font-size:80%;margin-top: .25rem; color:red;display:none;">Boş geçilemez !!</span>
    </div>
    <div class="form-group">
        <label class="control-label">Tarih Seç</label>
        <input class="form-control" type="text" id="datepicker" placeholder="Tarih Seç" autocomplete="off">
        <span id="alertdatepicker" style="font-size: 80%; margin-top: .25rem; color: red; display: none;">Boş geçilemez !!</span>
    </div>
    <div class="form-group">
        <label for="InputWkt">WKT</label>
        <input type="text" class="form-control" id="InputWkt" placeholder="WKT" readonly />
        <button id="addBtn" style="margin-top:10px;" type="button" class="btn btn-primary">Kaydet</button>
        <button id="cancelBtn" style="margin-top:10px;" type="button" class="btn btn-danger">İptal</button>
    </div>
</form>
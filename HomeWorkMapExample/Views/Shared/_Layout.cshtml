<!DOCTYPE html>
<html lang="tr">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Home Work Map Example</title>

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link href="~/css/ol.css" rel="stylesheet" />
    <link href="~/css/bootstrap-table.min.css" rel="stylesheet" />
    <link href="~/css/bootstrap-datepicker.css" rel="stylesheet" />
    <link href="~/lib/notifyMessage/css/notifyMessage.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jspanel3/3.11.0/jquery.jspanel.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/malihu-custom-scrollbar-plugin/3.1.5/jquery.mCustomScrollbar.min.css">
    <link rel="stylesheet" href="https://unpkg.com/ol-popup@4.0.0/src/ol-popup.css" />
    @*<link rel="stylesheet" href="~/css/site.css" />*@
    <link href="~/css/custom.css" rel="stylesheet" />

</head>
<body style="overflow:hidden;">

    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <a class="navbar-brand" href="#">BaseAPP v1.0.0</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Çizimler
                    </a>
                    <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                        <a class="dropdown-item draw-geom" href="javascript:void(0);" name="Point" value="Point">Nokta Ekle</a>
                        <a class="dropdown-item draw-geom" href="javascript:void(0);" name="LineString" value="LineString">Çizgi Ekle</a>
                        <a class="dropdown-item draw-geom" href="javascript:void(0);" name="Polygon" value="Polygon">Kapalı Alan Ekle</a>
                        <a id="addWkt" class="dropdown-item" href="javascript:void(0);">WKT'den Ekle</a>
                    </div>
                </li>
                <li class="nav-item">
                    <a class="nav-link get-info" name="display-save-list" href="javascript:void(0);">Listele</a>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="javascript:void(0);" data-toggle="dropdown">Altlıklar</a>
                    <div class="dropdown-menu">
                        <h6 class="dropdown-header">Google</h6>
                        <a class="dropdown-item tile-layer" href="javascript:void(0);" name="http://mt0.google.com/vt/lyrs=m&hl=en&x={x}&y={y}&z={z}">Roadmap</a>
                        <a class="dropdown-item tile-layer" href="javascript:void(0);" name="http://mt0.google.com/vt/lyrs=p&hl=en&x={x}&y={y}&z={z}">Terrain</a>
                        <a class="dropdown-item tile-layer" href="javascript:void(0);" name="http://mt0.google.com/vt/lyrs=r&hl=en&x={x}&y={y}&z={z}">Altered roadmap</a>
                        <a class="dropdown-item tile-layer" href="javascript:void(0);" name="http://mt0.google.com/vt/lyrs=s&hl=en&x={x}&y={y}&z={z}">Satellite only</a>
                        <a class="dropdown-item tile-layer" href="javascript:void(0);" name="http://mt0.google.com/vt/lyrs=t&hl=en&x={x}&y={y}&z={z}">Terrain only</a>
                        <a class="dropdown-item tile-layer" href="javascript:void(0);" name="http://mt0.google.com/vt/lyrs=y&hl=en&x={x}&y={y}&z={z}">Hybrid</a>
                        <h6 class="dropdown-header">OSM</h6>
                        <a class="dropdown-item tile-layer" href="javascript:void(0);" name="https://tile.openstreetmap.org/{z}/{x}/{y}.png">Standart</a>
                    </div>
                </li>
                <li class="nav-item">
                    <a id="projDefine" class="nav-link" name="display-save-list" href="javascript:void(0);">Projeksiyon Tanımla</a>
                </li>
                <li class="nav-item">
                    <a id="dataEntry" class="nav-link" name="display-save-list" href="javascript:void(0);">Veri Gir</a>
                </li>
                <li class="nav-item">
                    <a id="mapClear" class="nav-link" name="display-save-list" href="javascript:void(0);">Haritayı Temizle</a>
                </li>
                <li class="nav-item">
                    <input type="file" class="custom-file-input" style="display:none;" id="addKML">
                    <a class="nav-link" href="javascript:void(0);" data-toggle="dropdown" onclick="document.getElementById('addKML').click();">KML Dosyası Ekle</a>
                </li>
                <li class="nav-item">
                    <a id="drawAll" class="nav-link" name="display-save-list" href="javascript:void(0);">Hepsini Çiz</a>
                </li>
            </ul>
        </div>
    </nav>

    <div class="main-content">
        @RenderBody()
    </div>

    <script src="~/js/jquery.min.js"></script>
    <!-- Option 1: Bootstrap Bundle with Popper -->
    @*<script src="~/js/bootstrap.bundle.min.js"></script>*@
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/proj4js/2.3.12/proj4.js" type="text/javascript"></script>
    <script src="~/js/ol.js"></script>
    <script src="https://unpkg.com/ol-popup@4.0.0"></script>
    <script src="~/js/jquery.jspanel-compiled.min.js"></script>
    <script src="~/js/bootstrap-table.min.js"></script>
    <script src="~/js/bootstrap-datepicker.min.js"></script>
    <script src="~/js/bootstrap-datepicker.tr.min.js"></script>
    <script src="~/lib/notifyMessage/js/notifyMessage.min.js"></script>
    <script src="~/js/map.js"></script>
    <!-- jQuery Custom Scroller CDN -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/malihu-custom-scrollbar-plugin/3.1.5/jquery.mCustomScrollbar.concat.min.js"></script>

</body>
</html>
